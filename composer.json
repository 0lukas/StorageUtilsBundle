{
    "name": "vimex/storage-utils-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo Symfony Storage Utils Bundle",
    "keywords": ["Akeneo", "Storage", "Utils", "Doctrine", "ORM"],
    "homepage": "https://github.com/akeneo/StorageUtilsBundle",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "https://github.com/0lukas/StorageUtils" }
    ],
    "require": {
        "php": ">=7.1.0",
        "vimex/storage-utils": "dev-master",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/config": "^4.1",
        "symfony/options-resolver": "^4.1",
        "symfony/event-dispatcher": "^4.1",
        "doctrine/orm": "^2.6.1"
    },
    "require-dev": {
        "phpspec/phpspec": "3.0.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Akeneo\\Bundle\\StorageUtilsBundle\\": "" }
    },
    "minimum-stability": "dev"
}
